    <!-- Compiled and minified JavaScript offline Materialize -->
    <script src="{{ asset('bundles/corinne/js/materialize.min.js') }}"></script>

    {% block js %}{% endblock %}

    <script type="text/javascript" src="{{ asset('bundles/corinne/js/app.js') }}"></script>

</body>

</html>
